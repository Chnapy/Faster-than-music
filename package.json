{
  "name": "faster-than-music",
  "version": "0.0.1",
  "description": "A game about a man who fight for his life.",
  "main": "src/main.ts",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "prestart": "npm run build",
    "start": "electron ./dist/main.js",
    "pretest": "webpack --config webpack.tests.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chnapy/Faster-than-music.git"
  },
  "keywords": [
    "game",
    "music"
  ],
  "author": "Richard Haddad <contact@richardhaddad.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chnapy/Faster-than-music/issues"
  },
  "homepage": "https://github.com/Chnapy/Faster-than-music#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "webpack": "^3.10.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-loader": "^3.5.3",
    "ts-loader": "^3.2.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "standard": "^10.0.3",
    "standard-loader": "^6.0.1",
    "electron": "^1.7.10",
    "html-webpack-plugin": "^2.30.1",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "jest": "^22.1.4",
    "jest-junit": "^3.4.1",
    "@types/jest": "^22.0.1"
  },
  "engines": {
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.jsx?",
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./TEST-jest_junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  }
}
